{
  "name": "ng2-release-webpack",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "rimraf": "rimraf",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "ngc": "ngc",
    "build:dev": "npm run rimraf -- build-dev && webpack --config ./config/webpack.config.js --env.is_production=0 --progress --profile --colors --display-error-details --display-cached --bail",
    "build:prod": "npm run rimraf -- dist && webpack --config ./config/webpack.config.js --env.is_production=1 --progress --profile --colors --display-error-details --display-cached --bail",
    "build:prod_aot": "npm run rimraf -- dist && ngc -p tsconfig.aot.json && webpack --config ./config/webpack.config.js --env.is_aot=1 --env.is_production=1 --progress --profile --colors --display-error-details --display-cached --bail",
    "server:dev": "webpack-dev-server --config ./config/webpack.config.js --env.is_production=0 --profile --colors --display-error-details --content-base ./build-dev --inline --bail",
    "server:prod": "webpack-dev-server --config ./config/webpack.config.js --env.is_production=1 --profile --colors --display-error-details --content-base ./dist --inline --bail",
    "jsonserver": "json-server --watch api-server-mock.json",
    "test": "karma start config/karma.conf.js"
  },
  "dependencies": {
    "@angular/common": "^2.4.7",
    "@angular/compiler": "^2.4.7",
    "@angular/core": "^2.4.7",
    "@angular/forms": "^2.4.7",
    "@angular/http": "^2.4.7",
    "@angular/platform-browser": "^2.4.7",
    "@angular/platform-browser-dynamic": "^2.4.7",
    "@angular/platform-server": "^2.4.7",
    "@angular/router": "~3.4.7",
    "@angular/upgrade": "^2.4.7",
    "@ngrx/core": "^1.2.0",
    "@ngrx/store": "^2.2.1",
    "core-js": "^2.4.1",
    "jquery": "^3.1.0",
    "lodash": "^4.17.4",
    "redux": "^3.6.0",
    "reflect-metadata": "^0.1.8",
    "rxjs": "^5.0.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.4.7",
    "@types/core-js": "^0.9.35",
    "@types/jasmine": "^2.5.37",
    "@types/node": "^6.0.46",
    "@types/source-map": "^0.1.29",
    "@types/uglify-js": "^2.6.28",
    "@types/webpack": "^1.12.36",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^2.2.4",
    "codelyzer": "0.0.28",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.19.0",
    "istanbul-instrumenter-loader": "^1.0.0",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.5.0",
    "json-loader": "^0.5.4",
    "json-server": "^0.9.4",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-source-map-support": "^1.2.0",
    "karma-webpack": "^1.8.0",
    "node-sass": "^4.5.0",
    "protractor": "3.3.0",
    "raw-loader": "^0.5.1",
